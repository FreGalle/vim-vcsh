{
  "*.go": {
    "*.go": {
      "alternate": "{}_test.go",
      "type": "source"
    },
    "*_test.go": {
      "alternate": "{}.go",
      "type": "test"
    }
  },
  "*.sql": {
    "*.up.sql": {
      "alternate": "{}.down.sql",
      "type": "up"
    },
    "*.down.sql": {
      "alternate": "{}.up.sql",
      "type": "down"
    }
  }
}
